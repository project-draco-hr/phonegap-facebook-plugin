{
  checkOffsetAndCount(buffer.length,offset,count);
  checkNotClosed();
  if (in == null || inputExhausted) {
    return -1;
  }
  int read=in.read(buffer,offset,count);
  if (read == -1) {
    inputExhausted=true;
    endOfInput(false);
    return -1;
  }
  cacheWrite(buffer,offset,read);
  return read;
}
