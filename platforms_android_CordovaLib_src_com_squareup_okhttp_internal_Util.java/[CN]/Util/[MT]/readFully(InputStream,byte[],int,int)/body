{
  if (byteCount == 0) {
    return;
  }
  if (in == null) {
    throw new NullPointerException("in == null");
  }
  if (dst == null) {
    throw new NullPointerException("dst == null");
  }
  checkOffsetAndCount(dst.length,offset,byteCount);
  while (byteCount > 0) {
    int bytesRead=in.read(dst,offset,byteCount);
    if (bytesRead < 0) {
      throw new EOFException();
    }
    offset+=bytesRead;
    byteCount-=bytesRead;
  }
}
