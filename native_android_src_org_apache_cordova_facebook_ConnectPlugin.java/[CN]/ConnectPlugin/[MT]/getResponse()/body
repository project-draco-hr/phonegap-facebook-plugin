{
  String response;
  if (facebook.isSessionValid()) {
    response="{" + "\"status\": \"connected\"," + "\"authResponse\": {"+ "\"accessToken\": \"" + facebook.getAccessToken() + "\","+ "\"expiresIn\": \""+ facebook.getAccessExpires()+ "\","+ "\"session_key\": true,"+ "\"sig\": \"...\","+ "\"userId\": \""+ this.userId+ "\""+ "}"+ "}";
  }
 else {
    response="{" + "\"status\": \"unknown\"" + "}";
  }
  try {
    return new JSONObject(response);
  }
 catch (  JSONException e) {
    e.printStackTrace();
  }
  return new JSONObject();
}
