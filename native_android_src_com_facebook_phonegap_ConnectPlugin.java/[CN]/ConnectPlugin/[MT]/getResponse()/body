{
  String response="{" + "    \"status\": \"" + (facebook.isSessionValid() ? "connected" : "unknown") + "\","+ "    \"session\": {"+ "        \"access_token\": \""+ facebook.getAccessToken()+ "\","+ "        \"expires\": \""+ facebook.getAccessExpires()+ "\","+ "        \"secret\": \"b082c4620cdac27e0371f2c674026662\","+ "        \"session_key\": true,"+ "        \"sig\": \"...\","+ "        \"uid\": \""+ this.userId+ "\""+ "    }"+ "}";
  try {
    return new JSONObject(response);
  }
 catch (  JSONException e) {
    e.printStackTrace();
  }
  return new JSONObject();
}
