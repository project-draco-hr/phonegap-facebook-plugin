{
  Bundle params=new Bundle();
  if (s != null) {
    String array[]=s.split("&");
    for (    String parameter : array) {
      String v[]=parameter.split("=");
      if (v.length == 2) {
        params.putString(URLDecoder.decode(v[0]),URLDecoder.decode(v[1]));
      }
    }
  }
  return params;
}
