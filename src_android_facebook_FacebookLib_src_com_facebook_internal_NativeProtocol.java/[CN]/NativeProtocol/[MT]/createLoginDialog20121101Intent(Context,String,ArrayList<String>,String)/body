{
  Intent intent=findActivityIntent(context,INTENT_ACTION_PLATFORM_ACTIVITY,ACTION_LOGIN_DIALOG);
  if (intent == null) {
    return null;
  }
  intent.putExtra(EXTRA_PROTOCOL_VERSION,PROTOCOL_VERSION_20121101).putExtra(EXTRA_PROTOCOL_ACTION,ACTION_LOGIN_DIALOG).putExtra(EXTRA_APPLICATION_ID,applicationId).putStringArrayListExtra(EXTRA_PERMISSIONS,ensureDefaultPermissions(permissions)).putExtra(EXTRA_PROTOCOL_CALL_ID,generateCallId()).putExtra(EXTRA_WRITE_PRIVACY,ensureDefaultAudience(audience));
  return intent;
}
