{
  Intent intent=new Intent().setAction(INTENT_ACTION_PLATFORM_ACTIVITY).setPackage(FACEBOOK_PACKAGE).addCategory(Intent.CATEGORY_DEFAULT).putExtra(EXTRA_PROTOCOL_VERSION,PROTOCOL_VERSION_20121101).putExtra(EXTRA_PROTOCOL_ACTION,ACTION_LOGIN_DIALOG).putExtra(EXTRA_APPLICATION_ID,applicationId).putStringArrayListExtra(EXTRA_PERMISSIONS,ensureDefaultPermissions(permissions)).putExtra(EXTRA_PROTOCOL_CALL_ID,generateCallId()).putExtra(EXTRA_WRITE_PRIVACY,ensureDefaultAudience(audience));
  return validateKatanaActivityIntent(context,intent);
}
